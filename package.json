{
  "name": "mdc-stencil",
  "version": "0.0.1",
  "description": "Stencils for Material Components for the web",
  "module": "dist/esm/index.js",
  "main": "dist/index.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "jest",
    "test.watch": "jest --watch"
  },
  "dependencies": {
    "@material/button": "^0.39.1",
    "@stencil/sass": "^0.1.0"
  },
  "devDependencies": {
    "@stencil/core": "0.12.4",
    "@types/jest": "^23.3.1",
    "jest": "^23.4.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qorsmond/mdc-stencil.git"
  },
  "author": "Quintin Orsmond",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/qorsmond/mdc-stencil/issues"
  },
  "homepage": "https://github.com/qorsmond/mdc-stencil",
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
