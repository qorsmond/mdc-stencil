{
  "name": "mdc-stencil",
  "version": "0.0.4",
  "description": "Material Design implemented using the Stencil Compiler for Web Components",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/mdc-stencil/mdc-stencil.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qorsmond/mdc-stencil.git"
  },
  "author": "Quintin Orsmond",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/qorsmond/mdc-stencil/issues"
  },
  "homepage": "https://github.com/qorsmond/mdc-stencil",
  "dependencies": {
    "@material/button": "^2.3.0",
    "@material/icon-button": "2.3.0",
    "@material/ripple": "2.3.0",
    "@material/tab": "^2.3.0",
    "@material/tab-bar": "^2.3.0",
    "@material/tab-indicator": "^2.3.0",
    "@material/tab-scroller": "^2.3.0"
  },
  "devDependencies": {
    "@stencil/core": "1.0.1",
    "@stencil/sass": "^0.2.3",
    "@types/jest": "^24.0.13"
  }
}
