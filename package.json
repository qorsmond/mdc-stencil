{
  "name": "mdc-stencil",
  "version": "0.0.2",
  "description": "Material Design implemented using the Stencil Compiler for Web Components",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/mdc-stencil/mdc-stencil.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll"
  },
  "devDependencies": {
    "@stencil/core": "1.0.0-beta.8",
    "@stencil/sass": "^0.2.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qorsmond/mdc-stencil.git"
  },
  "author": "Quintin Orsmond",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/qorsmond/mdc-stencil/issues"
  },
  "homepage": "https://github.com/qorsmond/mdc-stencil"
}
